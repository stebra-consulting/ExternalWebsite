@model List<ExternalWebsite2.Models.StebraEntity>

@{
    ViewBag.Title = "Nyheter";
}

<div class="row">
    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <div class="container-fluid news">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <div class="col-md-12">
                            <h2>@item.Title</h2>
                        </div>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <p>
                                @Html.Raw(item.Body)
                            </p>
                            <p><i>@item.Date @Html.Raw(item.Article)</i></p>
                        </div>
                    </div>
                </div>
            </div>
        }
    }
</div>